<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>slogin</servlet-name>
        <servlet-class>Control.slogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ssignup</servlet-name>
        <servlet-class>Control.ssignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sekview</servlet-name>
        <servlet-class>View.sekview</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>empview</servlet-name>
        <servlet-class>View.empview</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>postjob</servlet-name>
        <servlet-class>Control.postjob</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>applyjob</servlet-name>
        <servlet-class>Control.applyjob</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>feedbacksubmit</servlet-name>
        <servlet-class>View.feedbacksubmit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newfriend</servlet-name>
        <servlet-class>Control.newfriend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>slogin</servlet-name>
        <url-pattern>/slogin.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ssignup</servlet-name>
        <url-pattern>/ssignup.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sekview</servlet-name>
        <url-pattern>/sekview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>empview</servlet-name>
        <url-pattern>/empview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>postjob</servlet-name>
        <url-pattern>/postjob.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>applyjob</servlet-name>
        <url-pattern>/applyjob.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feedbacksubmit</servlet-name>
        <url-pattern>/feedbacksubmit.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newfriend</servlet-name>
        <url-pattern>/newfriend.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
